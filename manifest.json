{
  "name": "seasonvar_history",
  "version": "0.3.2",
  "manifest_version": 2,
  "description": "Расширение для добавления функционала и небольшого изменения внешнего вида сайта seasonvar.ru",
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  "content_scripts": [
    {
      "matches": ["http://seasonvar.ru/*"],
      "css": ["css/inject.css"],
      "js": ["js/jquery-1.11.3.min.js", "js/webdb.js", "js/app.js", "js/inject.js"]
    }
  ],
  "web_accessible_resources": ["img/seasonvar_eye.png"]
}